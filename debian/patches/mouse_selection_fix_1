-    author=R.AUTHOR,
-    author_email=R.AUTHOR_EMAIL,
-    maintainer=R.MAINTAINER,
-    maintainer_email=R.MAINTAINER_EMAIL,
+    author=ak_data.author,
+    author_email=ak_data.author_email,
+    maintainer=ak_data.maintainer,
